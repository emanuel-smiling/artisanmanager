<mx:Object xmlns="*" xmlns:mx="http://www.adobe.com/2006/mxml">
	<mx:Script>
		<![CDATA[
			import org.papervision3d.objects.DisplayObject3D;
		
			public static function calculate(position:Array, displayObject3D:DisplayObject3D):void
			{
				displayObject3D.x = position[0];
				displayObject3D.y = position[1];
				displayObject3D.z = position[2];
				displayObject3D.rotationX = position[3];
				displayObject3D.rotationY = position[4];
				displayObject3D.rotationZ = position[5];
			}
		]]>
	</mx:Script>
</mx:Object>
